extends layout

block content
  .people-pannel
    - each p in people
      .person(data-name=p.google.name)
        a(href='/people/' + encodeURIComponent(p.google.email))
          .avatar-container
            img(src=p.google.avatar)
          .name-container
            h3.name
              cufon.cufon.cufon-canvas
                - var displayName = (p.name ? p.name : p.google.name)
                cufontext= displayName

    .spacer
    #people-join
      a(href='http://www.rosedu.org/aboutus/') Do you want to join the team?

  - if (user)
    .spacer
    .spacer
    h2 Activity log
    - each log in logs
      p= log.msg
